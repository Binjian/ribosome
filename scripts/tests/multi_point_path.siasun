NOP
// Multi-Point Path Following
// Demonstrates continuous path with different motion types
SETFRAME TF 1
SETFRAME UF 1
// Define waypoints
SET P[1] P[1]
SET P[2] P[2]
SET P[3] P[3]
SET P[4] P[4]
SET P[5] P[5]
SET P[6] P[6]
// Speed settings
SET I[1] 100
SET I[2] 50
SET I[3] 20
// Move to start (joint move, fast)
MOVJ P[1] V = 100 ACC = 100 CNT = 0
// Fast approach to first point
MOVJ P[2] V = 80 ACC = 80 CNT = 50
// Linear moves with continuous blending
MOVL P[3] V = 500 ACC = 50 CNT = 80
MOVL P[4] V = 500 ACC = 50 CNT = 80
MOVL P[5] V = 500 ACC = 50 CNT = 80
MOVL P[6] V = 500 ACC = 50 CNT = 0
// Precise positioning at P[6] (CNT=0 means stop)
MOVL P[6] V = 100 ACC = 30 CNT = 0
// Return path with different speeds
MOVL P[5] V = 300 ACC = 40 CNT = 50
MOVL P[4] V = 400 ACC = 50 CNT = 50
MOVL P[3] V = 500 ACC = 60 CNT = 50
MOVL P[2] V = 600 ACC = 70 CNT = 50
// Joint move back to home
MOVJ P[1] V = 100 ACC = 100 CNT = 0
END
