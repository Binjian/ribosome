{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "test_simple",
  "rules": {
    "prog": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "START"
        },
        {
          "type": "SYMBOL",
          "name": "NEWLINE"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "stmt"
          }
        },
        {
          "type": "STRING",
          "value": "END"
        }
      ]
    },
    "stmt": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "assign"
        },
        {
          "type": "SYMBOL",
          "name": "NEWLINE"
        }
      ]
    },
    "assign": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "var"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "var"
        }
      ]
    },
    "var": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "X"
        },
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "SYMBOL",
          "name": "INT"
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "INT": {
      "type": "PATTERN",
      "value": "[0-9]+"
    },
    "NEWLINE": {
      "type": "PATTERN",
      "value": "\\r?\\n"
    },
    "SPACE": {
      "type": "PATTERN",
      "value": " +"
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "SPACE"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}